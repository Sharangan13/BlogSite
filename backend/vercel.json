{
  "version": 2,
  "installCommand": "npm install --legacy-peer-deps",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },

    {
      "src": "src/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "/build/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/build/index.html" }
  ]
}
